{
  "name": "@pinon/eslint-config",
  "version": "1.0.10",
  "description": "This package provides PinOn's .eslintrc for Javascript and Typescript as an extensible shared config.",
  "main": "index.js",
  "repository": "https://github.com/PinOn-Inc/tslint-config-pinon",
  "scripts": {
    "release": "npm version patch -m 'Release %s'",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch --no-verify"
    }
  },
  "author": "Zizhao Wang <zizhao.wang@pinon.io>",
  "license": "MIT",
  "peerDependencies": {
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": ">= 5",
    "prettier": ">= 1.17.0",
    "typescript": ">= 3"
  }
}
